class Solution:
    def isValid(self, s: str) -> bool:
        if len(s)%2 == 1:
            return False
        
        lookup = {")":"(","}":"{","]":"["}
        
        stack = list()
        
        for ch in s:
            if ch in lookup:
                if not stack or stack[-1] != lookup[ch]:
                    return False
                stack.pop()
                
            else:
                stack.append(ch)

                
        return not stack
